<%- if controller_name != 'registrations' %>
  <%= link_to "Registrar-se", new_registration_path(resource_name) %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' %>
  <%= link_to "Esqueceu a senha?", new_password_path(resource_name) %><br />
<% end %>

<%- if controller_name != 'sessions' %>
  <%= link_to "Conecte-se", new_session_path(resource_name) %><br />
<% end %>



